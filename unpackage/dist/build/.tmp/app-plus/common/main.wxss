	display: -webkit-box;
	display: -webkit-flex;
	display: -ms-flexbox;
	display: flex;
	-webkit-box-align: center;
	-webkit-align-items: center;
	-ms-flex-align: center;
	    align-items: center;
	text-decoration: none;
	color: #353535;
	-webkit-tap-highlight-color: transparent;
}
.is-item-line:before {
	content: " ";
	position: absolute;
